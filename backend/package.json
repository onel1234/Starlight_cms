{
  "name": "star-light-cms-backend",
  "version": "1.0.0",
  "description": "Backend API for Star Light Construction Management System",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
